{
  "name": "@apiso/api-service-orchestrator",
  "packageManager": "yarn@4.3.1",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "packages/services/*"
  ],
  "scripts": {
    "start": "ts-node packages/cli/src/index.ts",
    "dev": "ts-node packages/commands/dev.ts",
    "format": "prettier --write \"**/*.{js,mjs,cjs,ts,tsx,json,css,md}\" && eslint . --fix",
    "prettier": "prettier --write \"**/*.{js,mjs,cjs,ts,tsx,json,css,md}\"",
    "lint": "eslint .",
    "test": "yarn workspaces foreach run test --parallel",
    "build": "yarn workspaces foreach run build",
    "clean": "rimraf ./packages/*/dist"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "inquirer": "^8.2.4",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@eslint/eslintrc": "^3.1.0",
    "@jest/transform": "^29.7.0",
    "@jest/types": "^29.6.3",
    "@swc/wasm": "^1.7.1",
    "@types/babel__core": "^7.20.5",
    "@types/eslint": "^8.56.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.12",
    "@types/prettier": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@typescript-eslint/typescript-estree": "^7.17.0",
    "@typescript-eslint/utils": "^7.17.0",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.14.54",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "node-notifier": "^10.0.1",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": "22.5.1"
  },
  "author": "Bernie Green",
  "license": "MIT"
}
