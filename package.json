{
  "name": "@apiso/api-service-orchestrator",
  "packageManager": "yarn@4.3.1",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "generate-package": "node tools/scripts/generate-package.cjs",
    "lint": "eslint . --ext .ts",
    "test": "yarn workspaces foreach run test --parallel",
    "build": "yarn workspaces foreach run build",
    "clean": "rimraf ./packages/*/dist"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "google-auth-library": "^9.11.0",
    "googleapis": "^140.0.1",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "commander": "^9.0.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "zx": "^7.2.3"
  },
  "engines": {
    "node": "22.5.1"
