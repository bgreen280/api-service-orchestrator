{
  "name": "@apiso/api-service-orchestrator",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "node --experimental-specifier-resolution=node --loader ts-node/esm packages/cli/src/index.ts",
    "format": "prettier --write \"**/*.ts\"",
    "generate-package": "node tools/scripts/generate-package.js",
    "lint": "eslint . --ext .ts",
    "test": "npm run test --workspaces --if-present",
    "build": "npm run build --workspaces --if-present"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.14.11",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "author": "Bernie Green",
  "license": "MIT"
}
